{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "removeComments": true,
    "skipLibCheck": true,
    "declaration": true,
    "noImplicitAny": false,
    "baseUrl": "./",
    "paths": {
      "@/*": ["packages/mlz-admin/src/*"],
      "@@/*": ["packages/mlz-admin/src/.umi/*"]
    },
  },
  "exclude": [
    "node_modules",
    "es",
    "lib",
    "dist",
  ]
}
