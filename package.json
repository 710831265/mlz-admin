{
  "name": "@mlz/admin",
  "version": "0.0.1",
  "description": "一套编程猫设计规范下的管理系统React组件库，基于Antd",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "build": "father-build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "module": "dist/es/index.esm.js",
  "main": "dist/lib/index.js",
  "typings": "dist/lib/src/index.d.ts",
  "files": [
    "dist"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "react": "^16.12.0",
    "@ant-design/pro-table": "^2.2.7",
    "lodash-es": "^4.17.15"
  },
  "peerDependencies": {
    "antd": "^4.1.5",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "dumi": "^1.0.6",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.2.1",
    "@types/lodash-es": "^4.17.3",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-plugin-jest": "^23.8.2",
    "father-build": "^1.17.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
}