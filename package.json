{
  "name": "@mlz/admin",
  "version": "0.0.2",
  "description": "一套编程猫设计规范下的管理系统React组件库，基于Antd",
  "keywords": [
    "react",
    "components",
    "codemao"
  ],
  "scripts": {
    "start": "dumi dev",
    "dist": "rm -rf es lib dist && npm run dist:cjs && npm run dist:es",
    "dist:es": "tsc -d -m esnext --outDir es --sourceMap false --inlineSources false",
    "dist:cjs": "tsc -d -m commonjs --outDir lib --sourceMap false --inlineSources false",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "build": "father-build",
    "build:docs": "dumi build",
    "lint": "npm run lint:ts && npm run lint:esFix",
    "lint:esFix": "npx eslint --ext tsx,ts,js src --fix",
    "lint:es": " npx eslint --ext tsx,ts,js src docs/demo",
    "lint:ts": "tsc --project tsconfig.lint.json",
    "pretty-quick": "pretty-quick",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "version": "node ./scripts/versioning"
  },
  "module": "es/index.js",
  "main": "lib/index.js",
  "typings": "lib/src/index.d.ts",
  "files": [
    "lib",
    "es",
    "LICENSE",
    "CHANGELOG.md",
    "typings.d.ts"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged && npm run pretty-quick"
    }
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-table": "^2.2.7",
    "antd": "^4.1.5",
    "axios": "^0.19.2",
    "lodash-es": "^4.17.15",
    "react": "^16.12.0"
  },
  "peerDependencies": {
    "antd": "^4.1.5",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.2.1",
    "@types/lodash-es": "^4.17.3",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react-hooks": "^4.0.2",
    "father-build": "^1.17.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "yorkie": "^2.0.0"
  },
  "bugs": {
    "url": "https://github.com/juicecube/mlz-admin/issues"
  },
  "homepage": "https://milobluebell.github.io/mytils"
}
