{
  "name": "@mlz/admin",
  "version": "0.5.77",
  "description": "一套遵循编程猫组件使用规范的管理系统React组件库",
  "license": "MIT",
  "keywords": [
    "react",
    "components",
    "antd"
  ],
  "scripts": {
    "start": "npm run clean && node ./scripts/genIndex && NODE_ENV=development dumi dev",
    "build": "node ./scripts/genIndex && NODE_ENV=production dumi build",
    "dist": "npm run clean && node ./scripts/genIndex && NODE_ENV=production father-build",
    "dist:es": "npx ttsc --project ./tsconfig.dist.json -d -m esnext --outDir es --sourceMap false --inlineSources false",
    "dist:cjs": "npx ttsc --project ./tsconfig.dist.json -d -m commonjs --outDir lib --sourceMap false --inlineSources false",
    "serve": "node ./scripts/serve",
    "lint": "node ./scripts/genIndex && npm run lint:ts && npm run lint:es",
    "lint:fix": "npm run lint:ts && npm run lint:es --fix",
    "lint:esFix": "npx eslint --ext tsx,jsx,ts src docs/demo --fix",
    "lint:es": "npx eslint --ext tsx,jsx,ts src docs/demo",
    "lint:ts": "tsc --project ./tsconfig.lint.json",
    "lint:style": "stylelint 'src/**/*.less' --syntax less",
    "test": "npm run clean && node ./scripts/genIndex && NODE_ENV=test dumi test",
    "test:coverage": "npm run clean && node ./scripts/genIndex && NODE_ENV=test dumi test --coverage",
    "test:update": "npm run clean && node ./scripts/genIndex && jest --clearCache && NODE_ENV=test dumi test --cache=false -u --coverage",
    "check-coverage": "npx istanbul check-coverage --statement 85 --branch -77",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "version": "AUTO=1 node ./scripts/versioning",
    "changelog": "node ./scripts/changelog",
    "clean": "rm -rf es lib dist .tsc-check coverage src/index.tsx",
    "relaunch": "npm run clean && rm -rf node_modules package-lock.json yarn.lock src/.umi && npm install",
    "rapper": "rapper --type normal --rapperPath \"/rapper\" --apiUrl \"http://rap2api.taobao.org/repository/get?id=252468&token=KBZdG9oC-vKXr8_cyJuF0IGs40ag5EQ9\" --rapUrl \"http://rap2.taobao.org\""
  },
  "module": "es/index.js",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "es",
    "LICENSE",
    "service"
  ],
  "gitHooks": {
    "pre-commit": "npx lint-staged && npx pretty-quick --staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "@mlz/axios": "^1.1.3",
    "antd": "^4.7.0",
    "axios": "^0.20.0",
    "lodash-es": "^4.17.15",
    "mytils": "^1.3.21"
  },
  "peerDependencies": {
    "antd": ">=4.6.2",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@sentry/browser": "^5.27.6",
    "@sentry/tracing": "^5.27.6",
    "@testing-library/react": "^11.2.2",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.13",
    "@types/lodash-es": "^4.17.3",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "@umijs/preset-react": "^1.6.4",
    "@umijs/test": "^3.2.19",
    "bundlesize": "^0.18.0",
    "compression": "^1.7.4",
    "dumi": "^1.0.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-babel": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-markdown": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "express": "^4.17.1",
    "father-build": "^1.18.4",
    "inquirer": "^7.3.3",
    "istanbul": "^0.4.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.3.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "pretty": "^2.0.0",
    "pretty-quick": "^2.0.1",
    "rap": "^1.1.5",
    "react": "^16.9.0",
    "react-copy-to-clipboard": "^5.0.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "simple-git": "^2.27.0",
    "stylelint": "^13.7.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "typescript": "~3.9.7",
    "typescript-transform-paths": "^2.0.1",
    "yorkie": "^2.0.0"
  },
  "bugs": {
    "url": "https://github.com/juicecube/mlz-admin/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juicecube/mlz-admin"
  },
  "homepage": "https://mlz-admin.com",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}